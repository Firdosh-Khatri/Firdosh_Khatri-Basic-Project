secret_number = 9
no_of_guesses = 0
guess_limit = 3
while no_of_guesses < guess_limit:
    guess = int(input("Guess: "))
    no_of_guesses += 1
    if guess == secret_number:
        print("You win")
        break
    elif no_of_guesses <= guess_limit - 1:
        print("Try again")
else:
    print("You failed")
