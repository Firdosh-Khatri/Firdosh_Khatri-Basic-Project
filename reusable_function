def emoji_convertor (message):
    word = message.split(' ')
    emojis = {
        ":)": "ğŸ˜€",
        ":(": "ğŸ˜’",
        "8-[": "ğŸ˜¡",
        "<3": "â¤ï¸"
    }
    output = ""
    for i in word:
        output += emojis.get(i, i) + " "
    return output


message = input(">")
print(emoji_convertor(message))
